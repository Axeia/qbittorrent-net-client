{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/QBittorrent.Client/QBittorrent.Client.csproj"
          ],
          "cwd": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "fusion/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "qBittorrent Client Library for .Net",
      "_appFooter": "Copyright Â© 2018 Pavel Fedarovich",
      "_appLogoPath": "images/icon.svg",
      "_appFaviconPath": "images/favicon.png"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}